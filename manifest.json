{
    "manifest_version": 2,
    "name": "Surviv.io Custom Sounds",
    "author":"humphreygaming, brzsmg and VN BPM",
    "version": "1.0.1",
    "description": "New sounds for surviv.io. Working with main web of surviv.io(https://surviv.io/)",
    "icons": {
        "128": "ext_128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "ext_128.png"
        }
    },
    "background": {
        "scripts": [ "background.js" ]
    },
    "web_accessible_resources":[
        "audio/*",
        "img/*"
    ],
    "permissions": [ "storage", "webRequest", "webRequestBlocking", "contextMenus", "https://surviv.io/" ]
}
